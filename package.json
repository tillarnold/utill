{
    "name": "utill",
    "version": "0.1.2",
    "homepage": "https://github.com/tillarnold/utill",
    "bugs": "https://github.com/tillarnold/utill/issues",
    "author": "Till Arnold",
    "description": "A utility library for nodejs and the browser",
    "repository": "https://github.com/tillarnold/utill",
    "main": "./index.js",
    "devDependencies": {
        "mocha": "~1.18.2",
        "should": "~3.3.1",
        "mocha-lcov-reporter": "0.0.1",
        "coveralls": "~2.10.0",
        "istanbul": "~0.2.8",
        "jshint": "~2.5.0"
    },
    "testling": {
        "harness": "mocha-tdd",
        "files": [
            "test/*.js",
            "test/browser/*.js"
        ],
        "browsers": [
            "ie/8..latest",
            "firefox/16..latest",
            "chrome/22.latest",
            "safari/5..latest",
            "opera/11..latest",
            "iphone/6",
            "ipad/6",
            "android-browser/latest"
        ]
    },
    "scripts": {
        "test": "npm run lintv && npm run test-coverage && npm run generate-istanbul-report",
        "test-coverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
        "test-coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
        "generate-istanbul-report": "istanbul report html",
        "lint": "jshint ./lib ./test ./index.js",
        "lintv": "jshint --verbose ./lib ./test ./index.js"
    }
}